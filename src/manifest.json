{
  "manifest_version": 2,
  "name": "Format with Prettier",
  "description": "Use prettier from your browser!",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "../node_modules/webextension-polyfill/dist/browser-polyfill.js",
      "background.ts"
    ]
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": ["activeTab", "contextMenus", "clipboardWrite"]
}
